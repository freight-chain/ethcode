{
  "name": "ethcode",
  "displayName": "ETHcode",
  "description": "Ethereum IDE for VS Code",
  "version": "0.1.8",
  "publisher": "quantanetwork",
  "categories": [
    "Debuggers",
    "Programming Languages"
  ],
  "keywords": [
    "solidity",
    "ethereum",
    "remix-tests",
    "dapp",
    "blockchain"
  ],
  "icon": "images/ethcode_tsp.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/0mkara/ethcode"
  },
  "engines": {
    "vscode": "^1.25.0",
    "node": ">=8.16.0"
  },
  "activationEvents": [
    "onCommand:ethcode.activate",
    "onCommand:ethcode.compile",
    "onCommand:ethcode.refactor",
    "onCommand:ethcode.runTest"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "ÎžTHcode",
      "properties": {
        "ethcode.keystore.keystorePath": {
          "type": "string",
          "default": "",
          "description": "Path to publicAddresses generated by ethcode",
          "scope":"resource"
        },
        "ethcode.grpc.serviceEndpoints": {
          "type":"object",
          "default": {
            "client-call": "cc.ethcode.de:50053",
            "remix-tests":"rt.ethco.de:50051",
            "remix-debug":"rd.ethco.de:50052"
          },
          "description": "List of links to connect to ethcode backend",
          "scope": "application"
        },
        "ethcode.userConfig.sessionTimeStamp": {
          "type":"string",
          "default": "",
          "description": "Timestamp of last user session",
          "scope": "resource"
        },
        "ethcode.userConfig.defaultAccount": {
          "type": "string",
          "default": "",
          "description": "Default account for transactions",
          "scope": "resource"
        },
        "ethcode.userConfig.compiler": {
          "type":"object",
          "default": {
            "lang": "solidity",
            "solidityCompilerVersion": "latest"
          },
          "description": "Compiler configuration",
          "scope": "resource"
        },
        "ethcode.userConfig.networkId": {
          "type":"string",
          "default":"",
          "description": "Ethereum Network ID of operations",
          "scope": "application"
        },
        "ethcode.userConfig.gasStrategy": {
          "type":"string",
          "enum": [
            "high",
            "medium",
            "low"
          ],
          "default": "medium",
          "description": "Gas strategy for gas price during transactions",
          "scope": "application"
        }
      }
    },
    "commands": [
      {
        "command": "ethcode.activate",
        "title": "Activate ETHcode extension",
        "category": "Ethereum"
      },
      {
        "command": "ethcode.compile",
        "title": "Solidity compile",
        "category": "Ethereum"
      },
      {
        "command": "ethcode.refactor",
        "title": "Solidity compile refactor",
        "category": "Ethereum"
      },
      {
        "command": "ethcode.runTest",
        "title": "Solidity unit test",
        "category": "Ethereum"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+alt+e",
        "command": "ethcode.activate"
      },
      {
        "key": "ctrl+alt+c",
        "command": "ethcode.compile"
      },
      {
        "key": "ctrl+alt+t",
        "command": "ethcode.runTest"
      }
    ]
  },
  "dependencies": {
    "@babel/core": "7.4.3",
    "@grpc/grpc-js": "^0.6.9",
    "@grpc/proto-loader": "^0.5.2",
    "@primer/octicons-react": "^9.1.1",
    "@svgr/webpack": "4.1.0",
    "@types/randombytes": "^2.0.0",
    "@types/react-select": "^3.0.10",
    "@types/react-tabs": "^2.3.1",
    "@types/underscore": "^1.9.4",
    "@types/uuid": "^3.4.6",
    "axios": "^0.19.1",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.0",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "2.1.1",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "ethereumjs-tx": "^2.1.2",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "keythereum": "^1.0.4",
    "mini-css-extract-plugin": "0.5.0",
    "number-to-bn": "^1.7.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "react-diff-viewer": "^2.0.6",
    "react-dom": "^16.8.6",
    "react-json-pretty": "^2.2.0",
    "react-redux": "^7.0.3",
    "react-select": "^2.4.3",
    "react-tabs": "^3.0.0",
    "react-tiny-virtual-list": "^2.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "remix-url-resolver": "^0.0.12",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "semver": "6.0.0",
    "shelljs": "^0.8.3",
    "solc": "0.6.4",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "ts-pnp": "1.1.2",
    "typescript": "3.4.5",
    "underscore": "^1.9.2",
    "url-loader": "1.1.2",
    "utf8": "^3.0.0",
    "uuid": "^3.4.0",
    "web3-core-helpers": "^1.2.6",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.2.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "^12.0.2",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.1.0",
    "@types/redux-logger": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "copy-webpack-plugin": "^5.0.4",
    "redux-logger": "^3.0.6",
    "vscode": "^1.1.34"
  },
  "scripts": {
    "vscode:prepublish": "node scripts/build.js && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "build": "node scripts/build.js && tsc -p tsconfig.extension.json",
    "build:stats": "node scripts/build.js --json > stats.json",
    "test": "node scripts/test.js",
    "watch": "tsc -watch -p ./"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "./src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": {
    "name": "0mkara",
    "email": "0mkar@protonmail.com"
  },
  "contributors": [
    {
      "name": "Ayan Banerjee",
      "email": "ayanb1999@gmail.com"
    },
    {
      "name": "Soumyanil Jana",
      "email": "soumyaniljana@gmail.com"
    }
  ]
}
